class Solution {
    public int areaOfMaxDiagonal(int[][] dimensions) {

        int n = dimensions.length;
        int maxdis = 0;
        int maxarea=0;
    for(int i=0;i<n;i++){
            int len = dimensions[i][0];
            int width = dimensions[i][1];
            int ans = ((len*len)+(width*width));
           int area = len*width;

           if(ans> maxdis){
            maxdis = ans;
            maxarea = area;
           }
           else if(ans == maxdis){
            maxarea = Math.max(maxarea,area);
           }
    }
    return maxarea;


    }
}
