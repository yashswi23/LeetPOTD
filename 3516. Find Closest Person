class Solution {
    public int findClosest(int x, int y, int z) {
        int x_dis = Math.abs(z-x);
        int y_dis = Math.abs(z-y);

        if(x_dis < y_dis){
            return 1;
        }
        else if(x_dis> y_dis){
            return 2;
        }
        else{
            return 0;
        }
    }
}
